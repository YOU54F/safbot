{
    "name": "safbot",
    "version": "0.0.3",
    "description": "various cli tools I find useful, all rolled up in one",
    "main": "index.js",
    "bin": {
        "saf": "./dist/shell.js",
        "safbot": "./dist/shell.js"
    },
    "scripts": {
        "prebuild": "rimraf ./node_modules/newman-collection/index.ts",
        "build": "npm run prebuild && tsc --skipLibCheck",
        "test": "echo \"Error: no test specified\" && exit 1",
        "link": "yarn link saftest",
        "start": "yarn -px saftest",
        "commands": "ts-node ./src/shell.ts",
        "newman": "newman",
        "portman": "portman",
        "newman-slack": "./scripts/newman-to-slack.sh",
        "newman-wrapper": "newman-wrapper \"./samples/test.postman_environment.json\" \"host\" \"http://localhost:3002\"",
        "postman-cli": "postman",
        "postman-pact": "pmpact",
        "newman-collection": "ts-node ./src/newmanCollection.ts",
        "postman-k6": "postman-to-k6",
        "postman-collection-transformer": "postman-collection-transformer",
        "postman2openapi": "ts-node ./src/postman2oas.ts",
        "postman-to-openapi": "p2o",
        "openapi2postmanv2": "openapi2postmanv2",
        "openapi-format": "openapi-format",
        "oas-validate": "oas-validate",
        "swagger-to-sample-code": "ts-node ./src/swagger-to-sample-code.ts",
        "swagger2openapi": "swagger2openapi",
        "swagger-pact": "swagger-mock-validator",
        "pact-login": "ts-node ./src/cognitocli/runner.ts  --cognitoclient 7t2s56arpg424kh7ou60apca8m --userpool ap-southeast-2_x0L1olP0D --run 'curl https://saf.pactflow.io/settings/tokens' --header cookie",
        "pact-diagram": "ts-node ./src/pact-diagram.ts",
        "cy-openapi": "cy-openapi",
        "cy-openapi:test": "cy-openapi samples/oas-v3.0.1.yml -o cypress/integration --baseURL http://localhost:3000",
        "handlebars": "handlebars",
        "prism": "prism",
        "spectral": "spectral",
        "curlx": "curlx",
        "postman2md": "postman2md",
        "postman2md:test": "postman2md ./samples/test.postman_collection.json ./samples/mdFromPostman.md",
        "pmac": "pmac",
        "pat": "pat",
        "xml2js": "xml2js",
        "js-yaml": "js-yaml",
        "postbox:split": "postbox-split",
        "postbox-merge": "postbox-merge",
        "redoc-cli": "redoc-cli",
        "wsdl-to-ts": "wsdl-to-ts",
        "wsdl-to-ts:test": "wsdl-to-ts \"https://www.w3schools.com/xml/tempconvert.asmx?WSDL\"",
        "openapi-diff": "openapi-diff",
        "pact-json-schema": "ts-node ./src/pactSchemas.ts"
    },
    "author": "Yousaf Nabi",
    "license": "ISC",
    "dependencies": {
        "@apideck/portman": "^1.16.1",
        "@apideck/postman-to-k6": "^1.8.3",
        "@blueworld/postbox": "^1.2.2",
        "@jc21/cypress-swagger-validation": "^0.1.0",
        "@matt.ball/postman-cli": "^0.1.2",
        "@pactflow/swagger-mock-validator": "^11.0.0",
        "@stoplight/prism-cli": "^4.9.1",
        "@stoplight/spectral-cli": "^6.3.0",
        "amazon-cognito-identity-js": "^5.2.8",
        "cli-diagram": "^0.3.0",
        "cli-ux": "6",
        "cross-fetch": "^3.1.5",
        "curlx": "^0.0.12",
        "cy-openapi": "^0.0.7",
        "ink": "^3.2.0",
        "json-schema-ref-parser": "^9.0.9",
        "newman-collection": "^1.5.1",
        "newman-reporter-htmlextra": "^1.22.8",
        "newman-wrapper": "^1.0.10",
        "node-persist": "^3.1.0",
        "oas-validator": "^5.0.8",
        "openapi-diff": "^0.23.5",
        "openapi-to-postmanv2": "^3.1.0",
        "pact-json-schema": "^0.2.0",
        "pat-cli": "^0.2.3",
        "pmac": "^1.4.0",
        "pmpact": "^0.3.14",
        "postman-collection-transformer": "^4.1.6",
        "postman-to-openapi": "^2.2.1",
        "postman2md": "^0.2.4",
        "postman2openapi": "^1.0.0-beta",
        "redoc-cli": "^0.13.10",
        "shelljs": "^0.8.5",
        "swagger-to-sample-code": "^1.0.1",
        "swagger2openapi": "^7.0.8",
        "wsdl-to-ts": "^0.2.3"
    },
    "devDependencies": {
        "@types/node": "17.0.25",
        "@types/shelljs": "0.8.11",
        "rimraf": "3.0.2",
        "ts-node": "10.7.0",
        "typescript": "4.6.3"
    }
}
